<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.library.new_library.mapper.DamagedBookMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.library.new_library.entity.DamagedBook">
        <id column="book_id" property="bookId" />
        <result column="user_id" property="userId" />
        <result column="damage_time" property="damageTime" />
        <result column="repair_time" property="repairTime" />
        <result column="borrow_time" property="borrowTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        book_id, user_id, damage_time, repair_time, borrow_time
    </sql>

</mapper>
